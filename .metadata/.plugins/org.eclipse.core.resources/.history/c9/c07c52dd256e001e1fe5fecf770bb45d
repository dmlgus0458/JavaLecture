package chap09_inherit.animal;

public class Tiger extends Animal {
	String color;
	
	public Tiger() {
		
	}
	
	public Tiger(int age, String size, int legCnt, boolean hasWing,
			String color) {
		this.age = age;
		this.size = size;
		this.legCnt = legCnt;
		this.hasWing = hasWing;
		this.color = color;
	}
	
	
}
